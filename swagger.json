      "name": "Domain2",
      "description": "Domain2 paths"
    }
  ],
  "schemes": [
    "https"
  ],
  "paths": {
    "/foo": {
      "post": {
        "tags": [
          "Domain1"
        ],
        "summary": "Store Foo",
        "description": "",
        "operationId": "foo.store",
        "consumes": [
          "application/x-www-form-urlencoded"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "foo",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "Foo name of the foo."
          }
        ],
        "responses": {
          "303": {
            "description": "Redirect to created /foo/{foo}",
            "schema": {
              "$ref": "#/definitions/Foo"
            }
          },
          "405": {
            "description": "Invalid input"
          }
        }
      }
    },
    "/foo/{foo}": {
      "get": {
        "tags": [
          "Domain1"
        ],
        "summary": "Returns the foo view.",
        "description": "",
        "operationId": "foo.show",
        "produces": [
          "text/html"
        ],
        "parameters": [
          {
            "name": "foo",
            "in": "path",
            "required": true,
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns foo view."
          },
          "403": {
            "description": "Not authorized."
          },
          "404": {
            "description": "Foo ID {foo} does not exist."
          }
        }
      }
    }
  },
  "definitions": {
    "Foo": {
      "type": "object",
      "properties": {
        "foo": {
          "type": "string"
        }
      }
    }
